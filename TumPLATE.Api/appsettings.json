{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "sampleDb": "Data Source=.;Initial Catalog=sample;TrustServerCertificate=True;Integrated Security=True",
    "hangfireDb": "Data Source=.;Initial Catalog=Hangfire;TrustServerCertificate=True;Integrated Security=True"
  },
  "AllowedHosts": "*",
  "FeatureFlags": {
    "r4_fruit": {
      "EnabledFor": [
        {
          "Name": "HostingEnvironment",
          "Parameters": {
            "ActivateOnEnvironments": [ "Development", "SIT", "Pre-Production","Production"]
          }
        }
      ]
    }
  }
}
